<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <queries>
        <package android:name="net.nyx.printerservice" />
    </queries>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/print"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/print"
        android:supportsRtl="true"
        android:theme="@style/Theme.LabelPrinter"
        tools:targetApi="31" >
        <activity
            android:name=".HomePhotoActivity"
            android:exported="false" />
        <activity
            android:name=".HomeReceiptActivity"
            android:exported="false" />
        <activity
            android:name=".HomePriceActivity"
            android:exported="false" />
        <activity
            android:name=".HomePDFActivity"
            android:exported="false" />
        <activity
            android:name=".HomeEditActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditPrintActivity"
            android:exported="false" />
    </application>

</manifest>